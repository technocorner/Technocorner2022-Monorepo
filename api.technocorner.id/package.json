{
  "name": "api.technocorner.id",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "export NODE_ENV=production && node dist/index.js",
    "dev": "export NODE_ENV=development && nodemon",
    "test": "export NODE_ENV=test && nodemon",
    "build": "tsc && grunt && cp -r src/assets dist",
    "build-deta": "cp -r dist deploy/deta && rm deploy/deta/dist/netlify.js",
    "build-netlify": "netlify-lambda --config ./netlify.webpack.function.js build dist && cp -r src/assets deploy/netlify && mv deploy/netlify/netlify.js deploy/netlify/index.js",
    "build-vercel": "cp -r dist deploy/vercel && rm deploy/vercel/dist/netlify.js",
    "build-heroku": "cp -r dist deploy/heroku && rm deploy/heroku/dist/netlify.js",
    "build-all": "concurrently \"npm run build-deta\" \"npm run build-netlify\" \"npm run build-vercel\" \"npm run build-heroku\"",
    "deploy-deta": "deta deploy deploy/deta",
    "deploy-netlify": "netlify deploy --prod",
    "deploy-vercel": "vercel --prod deploy/vercel",
    "deploy-all": "concurrently \"npm run deploy-deta\" \"npm run deploy-netlify\" \"npm run deploy-vercel\"",
    "rm-build-all": "rm -r tsconfig.tsbuildinfo dist/ deploy/deta/dist/ deploy/netlify/ deploy/vercel/dist/ deploy/heroku/dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebDev-Technocorner/api.technocorner.id.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WebDev-Technocorner/api.technocorner.id/issues"
  },
  "homepage": "https://github.com/WebDev-Technocorner/api.technocorner.id#readme",
  "dependencies": {
    "@sentry/node": "latest",
    "compression": "latest",
    "cookie-session": "latest",
    "cors": "latest",
    "csurf": "latest",
    "express": "latest",
    "express-actuator": "latest",
    "firebase-admin": "latest",
    "helmet": "latest",
    "keygrip": "latest",
    "multer": "latest",
    "nodemailer": "latest",
    "request": "latest",
    "write-excel-file": "latest"
  },
  "devDependencies": {
    "@firebase/app-compat": "latest",
    "@types/compression": "latest",
    "@types/cookie-session": "latest",
    "@types/cors": "latest",
    "@types/csurf": "latest",
    "@types/express": "latest",
    "@types/express-actuator": "latest",
    "@types/multer": "latest",
    "@types/nodemailer": "latest",
    "@types/request": "latest",
    "concurrently": "latest",
    "grunt": "latest",
    "grunt-contrib-uglify": "latest",
    "grunt-contrib-watch": "latest",
    "heroku": "latest",
    "netlify-cli": "latest",
    "netlify-lambda": "latest",
    "nodemon": "latest",
    "serverless-http": "latest",
    "ts-node": "latest",
    "tslint": "latest",
    "typescript": "latest",
    "uglify-js": "latest",
    "vercel": "latest",
    "webpack-node-externals": "latest"
  }
}
